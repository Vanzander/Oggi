{
  "rich-text#jeans-hombre-rt": {
    "props": {
        "textAlignment": "LEFT",
        "textPosition": "LEFT",
        "text": "**[Hombre](/hombre/jeans)**",
        "blockClass": "link-coleccion"

    }
  },
  "rich-text#jeans-mujer-rt": {
