  "menu-item#category-jeans": {
    "props": {
      "id": "menu-item-category-jeans",
      "type": "custom",
      "categoryId": 2,
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "text": "Jeans",
        "href":"/jeans"
      }
    },
    "blocks": ["vtex.menu@2.x:submenu#category-jeans"]
  },

  "vtex.menu@2.x:submenu#category-jeans": {
    "props": {
      "width": "100%",
      "blockClass": "desktop-header-submenu"
    },
    "children": ["flex-layout.row#category-jeans"]
  },
  "flex-layout.row#category-jeans": {
    "children": [
      "flex-layout.col#category-jeans",
      "flex-layout.col#category-jeans-image"
    ]
  },

 "flex-layout.col#category-jeans-image": {
    "children": [
        "flex-layout.col#category-jeans-image2",
        "flex-layout.col#category-jeans-image1"
    ],
    "props": {
        "blockClass": "submenu_banner"
    }
},
"flex-layout.col#category-jeans-image1": {
    "children": [
        "image#placeholderjh1"
    ],
    "props": {
        "blockClass": "submenu_banner_image"
    }
},
"flex-layout.col#category-jeans-image2": {
    "children": [
        "image#placeholderjh2"
    ],
    "props": {
        "blockClass": "submenu_banner_image"
    }
},

"image#placeholderjh1": {
    "props": {
        "src": "assets/images/menu-categoria/jeans_mujer.jpg",
        "maxHeight": "",
        "maxWidth": "",
        "blockClass": "menu-image",
        "link":{
            "url":"https://www.oggi.mx/mujer"
        }
    }
},
"image#placeholderjh2": {
    "props": {
